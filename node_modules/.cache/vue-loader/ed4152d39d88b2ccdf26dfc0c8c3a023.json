C,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;IACvC","file":"/Users/quabbala/Desktop/vue-fundamentosApp/src/components/Counter.vue","sourceRoot":"","sourcesContent":["<template>\n    <h2>{{ customTitle }} </h2>\n\n    <p> {{ counter }} <sup>2</sup> = {{ squareCounted }}</p>\n\n    <button @click=\"increaseCouter\">+1</button>\n    <button @click=\"decreaseCounter\">-1</button>\n\n</template>\n\n<script>\n\nexport default {\n    props: {\n        title: String,\n        start: {\n            type: Number,\n            default: 100\n        }\n    },\n    // name: 'Counter',\n\n    data() {\n        return {\n            counter: this.start\n        }\n    },\n    methods: {\n        getSquareValue() {\n            return this.counter * this.counter \n        },\n        increaseCouter() {\n            this.counter ++\n        },\n        decreaseCounter() {\n            this.counter--\n        }\n    },\n    computed: {\n        squareCounted() {\n            return this.counter * this.counter\n        },\n        customTitle() {\n            return this.title ? this.title : \"Counter\"\n        }\n    }\n}\n</script>\n\n<style>\n    button{\n        background-color: aquamarine;\n        border-radius: 3px;\n        color:#000;    \n        cursor: pointer;\n        transition: 0.3s ease-in-out;\n        margin: 0 5px;\n        padding: 5px 15px;\n    }\n\n    button:hiver {\n        background-color:rgb(100, 177, 151);         \n    }\n</style>\n"]}]}